    "weather_numerical_standardized = weather_scaler.fit_transform(weather_numerical)\n",
    "weather_df_6[[\"Temperature\", \"Humidity\"]] = weather_numerical_standardized\n",
    "### END CODE ABOVE THIS LINE ###"
